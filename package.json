{
  "name": "react-custom-boilerplate",
  "version": "1.0.0",
  "description": "react custom boilerplate with redux, react-router",
  "private": false,
  "scripts": {
    "start": "react-scripts start",
    "start:prod": "serve -s ./build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "husky": "^2.4.0",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.0"
  },
  "dependencies": {
    "antd": "^3.19.3",
    "axios": "^0.19.0",
    "core-js": "^3.1.3",
    "http-proxy-middleware": "^0.19.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "5.1.1",
    "react-router": "4.4.0-beta.8",
    "react-router-dom": "4.4.0-beta.8",
    "react-scripts": "2.1.8",
    "redux": "4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "2.3.0",
    "serve": "10.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
